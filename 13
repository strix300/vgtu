#include <stdlib.h>
#include <locale.h>
#include <stdio.h>
#include <math.h>
#include <string.h>
#define _USE_MATH_DEFINES
//_CRT_SECURE_NO_DEPRECATE//

void proc1()
{
	char Name[20], Snew[] = "Hello", h[20];
	scanf("%s", Name);
	for (int i = 0; i < 20; i++)
	{
		h[i] = '!';
	}
	printf("%s %s%s", Snew, Name,h);
}
int main()
{
	setlocale(LC_ALL, "RUS");
	proc1();
}
