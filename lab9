#include <stdlib.h>
#include <locale.h>
#include <stdio.h>
#include <math.h>
#define _USE_MATH_DEFINES
#define ROW 0
#define COL 0
//_CRT_SECURE_NO_DEPRECATE//
void proc1a()
{
	int row, col;
	printf("\n");
	for (row = 8; row > ROW; --row)
	{
		for (col = 9; col > COL + row; --col)
			printf("%5d", col * row);
		printf("\n");
	}
	system("pause");
}
void proc1b()
{
	int n, m = 1, count = 0;
	printf("\n");
	for (n = 30; n >= 26; n++)
	{
		count++;
		printf("%5d", n);
		if (count == m)
		{
			printf("\n");
			n = n - (m + 1);
			m++;
			count = 0;
		}
	}
	return 0;
}
void proc2()
{
	int v, x,x1=0,x2=0,i,i1;
	printf("введите число:");
	scanf("%d", &v);
	i1 = 1;
	for (i = 0; i < i1; i++)
	{
		i1++;
		x = v % 10;
		v /= 10;
		if (x % 2 == 0)
			x1 += x;
		else
			x2 += x;
		if (v == 0)
			break;
	}
	printf("%d", x1 * x2);
}
int main()
{
	setlocale(LC_ALL, "RUS");
	proc1a();
}
